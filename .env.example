# Example environment variables for the Travel Diary app

# Database connection string
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/travel_diary_db

# Flickr API credentials
FLICKR_API_KEY=your_flickr_api_key
FLICKR_API_SECRET=your_flickr_api_secret

# YouTube API credentials
YT_CLIENT_ID=your_youtube_client_id
YT_CLIENT_SECRET=your_youtube_client_secret

# NextAuth secrets (if used)
NEXTAUTH_SECRET=change_this_secret
NEXTAUTH_URL=http://localhost:3000